<!-- BEGIN: main -->
<link rel="stylesheet" href="{SCRIPT}lytebox/lytebox.css" type="text/css" media="screen" />
	   <script type="text/javascript" src="{SCRIPT}lytebox/lytebox.js"></script>

<div id="wrapper">

    <div class="banner_img">

	<div><img src="{IMAGE}users/banner_category.jpg" alt="Product" /></div>
  
    </div>

<div id="product">

<div id="breadcrumb"><a href="{HOST}/productview">Products</a>::<a href="#">Category</a> :: <a href="#">{DATA.CATEGORY}</a>
</div>





  <div id="product">

 
 <div id="productpanel1">
 
 <div id="imagesbg"> 
 <div class="imagesdisplay">
 	<table>
 		<tr>
		<td style="width:10%;"><a href="{DATA.VIEW}"  rel="lytebox[vacation]" ><img src="{DATA.CROP}" alt="Sub Category" /></a></td>
		</tr>
		</table>
 </div></div>
 <div class="imagestext"></div>
 </div>
 <!-- product panel 2 text start -->
 <div id="productpanel2">
 <div class="txt"><strong>Description </strong><br />
{DATA.LONG_DESCRIPTION}
</div><br/><br/><br/><br/><br/>
    <div id="table">
        <table width="100%" border="0" cellspacing="1" cellpadding="1" class="table_color">
        
        <tr> 
         <td style="width:35%;">Item Code:</td>
         <th>{DATA.CODE}</th>
        </tr>

        <tr> 
         <td>Your Price:</td>
         <th><span class="pricecolor">($){DATA.PRICE}</span></th>
        </tr>
        
        <tr> 
         <td>Quantity:</td>
         <th><input name=""  size="6" type="text" /></th>
        </tr> 
         <tr> 
         <td></td>
         <th><a class="addtocart" href="#">Buy</a></th>
        </tr>        
        </table>
    </div>
 
 
 </div>
 
  <!-- product panel 2 text end -->
  
  


</div>
 
 


</div>
</div>

